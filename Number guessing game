import random


print("🎲 Welcome to the Number Guessing Game!")
number = random.randint(1, 10)
guess = None

while guess != number:
    guess = int(input("Guess a number between 1 and 10: "))
    if guess < number:
        print("Too low, try again.")
    elif guess > number:
        print("Too high, try again.")
    else:
        print("🎉 Correct! You guessed it!")

